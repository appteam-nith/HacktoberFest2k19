#include<conio.h>
#include<stdio.h>
main()
{
	int i,j,max,min;
printf("ENTER SIZE OF ARRAY  \n");
scanf("%d",&j);
int arr[j];
for(i=0;i<j;i++)
{
printf("ENTER %d NO. ",i+1);
scanf("%d",&arr[i]);	}
max=arr[0];min=arr[0];
for(i=1;i<j;i++)
{
	if(max<arr[i])
		max=arr[i];
	if(min>arr[i])
	min=arr[i];
	
}
printf("LARGEST NO. %d\n  ",max);
printf("SMALLEST NO.  %d  ",min);
return 0;
}
